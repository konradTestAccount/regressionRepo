<script>
if(window.$) {
	$('.collapse-all').hide();
		$('.expand-all').click(function() {
	  	$('.accordion>dd').addClass('active');
	  	$('.accordion>dd>div').addClass('active');
	  	$('.button.excol').toggle();
	  	return false;
	});
	
	$('.collapse-all').click(function() {
  		$('.accordion>dd').removeClass('active');
  		$('.accordion>dd>div').removeClass('active');
  		$('.button.excol').toggle();
  		return false;
	});
}
</script>
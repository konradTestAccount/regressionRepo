<t4 type="content" output="selective-output" modifiers="" name="Show Lightbox and Send Emails" format="<script>jQuery(document).ready(function(){  var error = true;jQuery('.lightbox_show').click(function() {jQuery.each(jQuery('#prospectus-form .added'), function() {if(jQuery(this).css('display') == 'block') {error = false;return;}                    });if(error === true) {alert('Please choose an option');} else {if(jQuery('#prospectus_lb').length > 0) {showLightbox();} else {return true;}}return false;});function showLightbox() {jQuery.colorbox({inline:true, href:'#prospectus_lb', width:'500px'});}});</script>" />